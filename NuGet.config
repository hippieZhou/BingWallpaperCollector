<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<packageSources>
		<clear />
		<add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
		<add key="labs.windows" value="https://pkgs.dev.azure.com/dotnet/CommunityToolkit/_packaging/CommunityToolkit-Labs/nuget/v3/index.json" />
		<add key="community" value="https://pkgs.dev.azure.com/dotnet/CommunityToolkit/_packaging/CommunityToolkit-MainLatest/nuget/v3/index.json" />
	</packageSources>
	<packageSourceMapping>
		<!-- CommunityToolkit Labs packages -->
		<packageSource key="labs.windows">
			<package pattern="CommunityToolkit.Labs.*" />
		</packageSource>
		<!-- CommunityToolkit Main packages -->
		<packageSource key="community">
			<package pattern="CommunityToolkit.Mvvm" />
			<package pattern="CommunityToolkit.WinUI.*" />
		</packageSource>
		<!-- All other packages from nuget.org -->
		<packageSource key="nuget.org">
			<package pattern="*" />
		</packageSource>
	</packageSourceMapping>
	<disabledPackageSources>
		<clear />
	</disabledPackageSources>
</configuration>
