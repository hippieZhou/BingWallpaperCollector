    <PackageVersion Include="CommunityToolkit.Labs.WinUI.TokenView" Version="0.1.250828-build.2223" />
    <!-- CommunityToolkit Packages -->
    <PackageVersion Include="CommunityToolkit.Mvvm" Version="8.4.0" />
    <PackageVersion Include="CommunityToolkit.WinUI.Behaviors" Version="8.2.250717-build.1484" />
    <PackageVersion Include="CommunityToolkit.WinUI.Animations" Version="8.2.250717-build.1484" />
    <PackageVersion Include="CommunityToolkit.WinUI.Collections" Version="8.2.250717-build.1484" />
